    <script src="script.js"></script>
    <script>
        const player = document.getElementById('player');
        const item = document.getElementById('item');
        let playerPos = {x: 0, y: 0};

        // G√®re les d√©placements du joueur
        function movePlayer(dx, dy) {
            const mapSize = 400;
            const playerSize = 32;
            // Met √† jour la position du joueur
            playerPos.x += dx;
            playerPos.y += dy;
            // V√©rifie les limites de la carte
            playerPos.x = Math.max(0, Math.min(mapSize - playerSize, playerPos.x));
            playerPos.y = Math.max(0, Math.min(mapSize - playerSize, playerPos.y));
            // D√©place le joueur
            player.style.left = playerPos.x + 'px';
            player.style.top = playerPos.y + 'px';
            // V√©rifie la collision avec l'item
            checkItemCollision();
        }

        function checkItemCollision() {
            const itemPos = {x: parseInt(item.style.left, 10), y: parseInt(item.style.top, 10)};
            if (playerPos.x < itemPos.x + 32 &&
                playerPos.x + 32 > itemPos.x &&
                playerPos.y < itemPos.y + 32 &&
                playerPos.y + 32 > itemPos.y) {
                // Collision d√©tect√©e
                alert("Item collect√© !");
                item.remove(); // Supprime l'item
            }
        }

        // √âcoute les touches du clavier pour les d√©placements
        document.addEventListener('keydown', function(event) {
            switch (event.key) {
                case 'ArrowUp': movePlayer(0, -32); break;
                case 'ArrowDown': movePlayer(0, 32); break;
                case 'ArrowLeft': movePlayer(-32, 0); break;
                case 'ArrowRight': movePlayer(32, 0); break;
            }
        });
    </script>    
