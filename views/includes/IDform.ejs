<form class="w3-blue w3-round w3-card w3-padding w3-margin-bottom">
    <p class="w3-bar-item w3-button w3-padding"><i class="fa fa-user fa-fw w3-margin-right"></i>Sign in</p>
    
    <input type="text" id="country" name="country" placeholder="⚓ Republic" value="" readonly><br>

    <input type="text" id="id_number" name="id_number" placeholder="🆔 Numéro" value="" readonly><br>

    <select id="nationality" name="nationality" disabled>
        <option value="0" disabled selected>🌍 Nationality</option>
        <option value="france">🇫🇷 France 🍷🥖</option>
        <option value="italy">🇮🇹 Italie 🍕🍝</option>
        <option value="japan">🇯🇵 Japon 🍣🗻</option>
        <option value="brazil">🇧🇷 Brésil 🕺🏞️</option>
        <option value="india">🇮🇳 Inde 🍛🕌</option>
        <option value="usa">🇺🇸 États-Unis 🍔🗽</option>
        <option value="canada">🇨🇦 Canada 🍁🏒</option>
        <option value="australia">🇦🇺 Australie 🦘🏜️</option>
        <option value="germany">🇩🇪 Allemagne 🍺🏰</option>
        <option value="spain">🇪🇸 Espagne 💃🥘</option>
    </select><br>

    <input type="text" id="pseudo" name="pseudo" placeholder="👤 Pseudo" value="" readonly><br>

    <input type="text" id="full_name" name="full_name" placeholder="👤 Nom(s) Prénom(s)" value="" readonly><br>

    <select id="gender" name="gender" disabled>
        <option value="" disabled selected>🚹🚺 Gender</option>
        <option value="chr23M">Male ♂</option>
        <option value="chr23F">Female ♀</option>
        <option value="0">False</option>
    </select><br>

    <input type="date" id="birthdate" name="birthdate" placeholder="🎂 Date de naissance" value="1990-01-01"><br>

    <input type="text" id="birthplace" name="birthplace" placeholder="📍 Né(e) à" value="" readonly><br>

    <input type="number" id="height" name="height" placeholder="📏 Taille" value="1.72" min="0.50" max="2.50" step="0.01" readonly><br>

    <!-- Onlick Add to database-->
    <input type="button" value="Scan"><br>

    <input type="number" id="weight" name="weight" placeholder="⚖️ Poids" value="70" min="20" max="200" step="0.5"><br>

    <select id="marital_status" name="marital_status">
        <option value="" disabled selected>💍 Statut marital</option>
        <option value="single">Célibataire</option>
        <option value="married">Marié(e)</option>
        <option value="divorced">Divorcé(e)</option>
    </select><br>

    <input type="text" id="address" name="address" placeholder="🧭 GPS" value="" readonly><br>
</form>


<form class="">

    <select class="w3-padding w3-blue w3-border" id="mbti" name="mbti" title="🧠"><!-- MBTI Personality Types -->
    <option value="intj">🧐 INTJ - Architect</option>
    <option value="intp">🤔 INTP - Logician</option>
    <option value="entj">👔 ENTJ - Commander</option>
    <option value="entp">🚀 ENTP - Debater</option>
    <option value="infj">🌟 INFJ - Advocate</option>
    <option value="infp">💭 INFP - Mediator</option>
    <option value="enfj">👥 ENFJ - Protagonist</option>
    <option value="enfp">🎉 ENFP - Campaigner</option>
    <option value="istj">📋 ISTJ - Logistician</option>
    <option value="isfj">🛡️ ISFJ - Defender</option>
    <option value="estj">👨‍💼 ESTJ - Executive</option>
    <option value="esfj">👨‍👩‍👦 ESFJ - Consul</option>
