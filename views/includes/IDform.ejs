    <option value="esfp">🎭 ESFP - Entertainer</option>
    </select>
    <br>

    <div class="w3-card"><!-- Chromosomes 🧬-->
    <input id="chr1" type="text" value="🧬" title="Principal porteur de gènes humains.">
    <input id="chr2" type="text" value="🔬" title="Fusion de deux chromosomes ancestraux.">
    <input id="chr3" type="text" value="🌐" title="Impliqué dans la régulation du système nerveux.">
    <input id="chr4" type="text" value="🍀" title="Lié à la susceptibilité de certaines maladies.">
    <input id="chr5" type="text" value="🌟" title="Contient des régions associées au développement.">
    <input id="chr6" type="text" value="🌙" title="Joue un rôle dans la réponse immunitaire.">
    <input id="chr7" type="text" value="✨" title="Comprend des gènes influençant les troubles sensoriels.">
    <input id="chr8" type="text" value="🎯" title="Implication dans le métabolisme énergétique.">
    <input id="chr9" type="text" value="🌱" title="Associé à la reproduction et aux cellules sanguines.">
    <input id="chr10" type="text" value="🔥" title="Comprend des gènes clés pour la fonction musculaire.">
    <input id="chr11" type="text" value="🌊" title="Important pour la production d'hémoglobine.">
    <input id="chr12" type="text" value="🌳" title="Contient des gènes essentiels pour la croissance.">
    <input id="chr13" type="text" value="🌈" title="Porte des gènes pour la vue et l'audition.">
    <input id="chr14" type="text" value="🎈" title="Vital pour le développement du système nerveux.">
    <input id="chr15" type="text" value="⚡" title="Impliqué dans la synthèse de certaines hormones.">
    <input id="chr16" type="text" value="🎨" title="Joue un rôle dans la digestion et l'énergie.">
    <input id="chr17" type="text" value="🌺" title="Important pour la régulation cellulaire.">
    <input id="chr18" type="text" value="🎉" title="Associé à la protection contre les maladies.">
    <input id="chr19" type="text" value="⚽" title="Contient des gènes pour le métabolisme des lipides.">
    <input id="chr20" type="text" value="🐠" title="Implication dans le contrôle du cycle cellulaire.">
    <input id="chr21" type="text" value="🚀" title="Lié au syndrome de Down.">
    <input id="chr22" type="text" value="🐉" title="Joue un rôle dans le système immunitaire et les maladies.">
    <input id="chrX" type="text" value="💕" title="Détermine les caractéristiques sexuelles (Chromosome X).">
    <input id="chrY" type="text" value="💕" title="Détermine les caractéristiques sexuelles (Chromosome Y).">
    </div>
    <br>

    <select class="" id="senses" name="senses"><!-- Senses 👁️‍🗨️-->
    <option value="sight">👀 Sight</option>
    <option value="hearing">👂 Hearing</option>
